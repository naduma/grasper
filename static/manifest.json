{
  "name": "grasper",
  "version": "1.1",
  "description": "grasper is a utility for organizing many open tabs.",
  "permissions": [
    "tabs",
    "windows"
  ],
  "icons" : {
    "16" : "icon/icon_16.png",
    "32" : "icon/icon_32.png",
    "48" : "icon/icon_48.png",
    "128" : "icon/icon_128.png"
  },
  "background":{
    "scripts":["background/background.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    }
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon_19.png"
    },
    "default_title": "grasper"
  },
  "manifest_version": 2
}
